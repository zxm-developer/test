{
  "name": "test-comit",
  "version": "1.0.0",
  "description": "test commit",
  "main": ".eslintrc.js",
  "scripts": {
    "commit": "eslint --ext .js,.vue ./src --fix  &&   ",
    "husky": "npx husky-init && npm install",
    "push": "git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxm-developer/test.git"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.(js|vue)": [
      "prettier --write",
      "eslint  --fix",
      "commitlint  --config commitlint.config.js -e"
    ]
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zxm-developer/test/issues"
  },
  "homepage": "https://github.com/zxm-developer/test#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "husky": "^8.0.0"
  },
  "dependencies": {
    "prettier": "^2.8.4"
  }
}
